    "type": "git",
    "url": "git+https://github.com/team-rideosoftware/ssl-certificates-cordova-plugin.git"
  },
  "keywords": [
    "Certificate",
    "SSL",
    "Pinning",
    "Man in the Middle",
    "MitM",
    "ecosystem:cordova",
    "cordova-ios",
    "cordova-android",
    "cordova-windows"
  ],
  "engines": [
    {
      "name": "cordova",
      "version": ">=3.0.0"
    }
  ],
