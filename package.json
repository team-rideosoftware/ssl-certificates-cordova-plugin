{
  "name": "cordova-plugin-ssl-certificates",
  "version": "6.0.0",
  "description": "This plugin allows allows you to get the SSL Certificates (serial numbers and fingerprints) of the server you're connecting to. This can be useful for prevention of Man In The Middle attacks.",
  "cordova": {
    "id": "cordova-plugin-ssl-certificates",
    "platforms": [
      "ios",
      "android",
      "windows"
    ]
  },
